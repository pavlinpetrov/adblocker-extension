{
    "name": "AdBlocker",
    "version": "1.0",
    "description": "Hides google paid search results and dir.bg ads.",
    "permissions": ["webRequest", "webRequestBlocking", "storage", "activeTab", "*://*.dir.bg/*", "*://*.doubleclick.net/*"],
    "background": {
        "scripts": ["utils.js", "background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://*.google.com/*"],
            "css": ["paid-results.css"],
            "js": ["utils.js", "paid-results.js"]
        }
    ],
    "manifest_version" : 2
}