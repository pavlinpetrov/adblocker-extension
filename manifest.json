{
    "name": "AdBlocker",
    "version": "1.0",
    "description": "Hides google paid search results and dir.bg ads.",
    "permissions": ["webRequest", "webRequestBlocking", "storage", "*://*.dir.bg/*", "*://*.google.com/*", "*://*.doubleclick.net/*"],
    "background": {
        "scripts": ["background.js"]
    },
    "manifest_version" : 2
}